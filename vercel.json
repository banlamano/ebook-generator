{
  "version": 2,
  "framework": null,
  "installCommand": "npm install && cd client && npm install",
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3",
      "includeFiles": "server/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ]
}
