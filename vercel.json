{
  "version": 2,
  "installCommand": "cd client && npm install --legacy-peer-deps",
  "buildCommand": "cd client && CI=false npm run build",
  "outputDirectory": "client/build",
  "framework": null,
  "trailingSlash": false,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 200,
      "dest": "/index.html"
    }
  ]
}
